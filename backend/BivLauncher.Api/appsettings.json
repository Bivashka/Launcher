{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=bivlauncher;Username=postgres;Password=postgres"
  },
  "PublicBaseUrl": "http://localhost:8080",
  "Branding": {
    "FilePath": "branding.json"
  },
  "Launcher": {
    "MinRamMb": 1024,
    "ReservedSystemRamMb": 1024
  },
  "LauncherUpdate": {
    "LatestVersion": "",
    "DownloadUrl": "",
    "ReleaseNotes": ""
  },
  "Hwid": {
    "HmacSalt": ""
  },
  "Jwt": {
    "Secret": "CHANGE_ME_TO_A_LONG_SECRET",
    "Issuer": "BivLauncher.Api",
    "Audience": "BivLauncher.Admin",
    "ExpireMinutes": 120
  },
  "S3": {
    "Endpoint": "http://localhost:9000",
    "Bucket": "launcher-files",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "ForcePathStyle": true,
    "UseSsl": false,
    "AutoCreateBucket": true
  },
  "BuildPipeline": {
    "SourceRoot": "BuildSources",
    "DefaultJvmArgs": "-Xms1024M -Xmx2048M",
    "DefaultGameArgs": ""
  },
  "NewsSync": {
    "Enabled": false,
    "IntervalMinutes": 60
  },
  "NewsRetention": {
    "Enabled": false,
    "MaxItems": 500,
    "MaxAgeDays": 30
  },
  "RuntimeRetention": {
    "Enabled": false,
    "IntervalMinutes": 360,
    "KeepLast": 3
  },
  "AuthProvider": {
    "LoginUrl": "",
    "TimeoutSeconds": 15,
    "AllowDevFallback": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
