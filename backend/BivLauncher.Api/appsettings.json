{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=bivlauncher;Username=postgres;Password=postgres"
  },
  "PublicBaseUrl": "http://localhost:8080",
  "Branding": {
    "FilePath": "branding.json"
  },
  "Launcher": {
    "MinRamMb": 1024,
    "ReservedSystemRamMb": 1024
  },
  "LauncherUpdate": {
    "FilePath": "launcher-update.json",
    "LatestVersion": "",
    "DownloadUrl": "",
    "ReleaseNotes": ""
  },
  "Hwid": {
    "HmacSalt": ""
  },
  "Jwt": {
    "Secret": "CHANGE_ME_TO_A_LONG_SECRET",
    "Issuer": "BivLauncher.Api",
    "Audience": "BivLauncher.Admin",
    "ExpireMinutes": 120,
    "PlayerExpireDays": 36500
  },
  "S3": {
    "UseS3": true,
    "LocalRootPath": "Storage",
    "Endpoint": "http://localhost:9000",
    "Bucket": "launcher-files",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "ForcePathStyle": true,
    "UseSsl": false,
    "AutoCreateBucket": true
  },
  "BuildPipeline": {
    "SourceRoot": "BuildSources",
    "DefaultJvmArgs": "-Xms1024M -Xmx2048M",
    "DefaultGameArgs": ""
  },
  "NewsSync": {
    "Enabled": false,
    "IntervalMinutes": 60
  },
  "NewsRetention": {
    "Enabled": false,
    "MaxItems": 500,
    "MaxAgeDays": 30
  },
  "RuntimeRetention": {
    "Enabled": false,
    "IntervalMinutes": 360,
    "KeepLast": 3
  },
  "AuthProvider": {
    "AuthMode": "external",
    "LoginUrl": "",
    "LoginFieldKey": "username",
    "PasswordFieldKey": "password",
    "TimeoutSeconds": 15,
    "AllowDevFallback": true
  },
  "DeveloperSupport": {
    "DisplayName": "Bivashka",
    "Telegram": "https://t.me/bivashka",
    "Discord": "bivashka",
    "Website": "https://github.com/bivashka",
    "Notes": "Official developer support contact. Not editable from admin UI."
  },
  "InstallTelemetry": {
    "Enabled": true
  },
  "DiscordRpc": {
    "Enabled": true,
    "PrivacyMode": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
